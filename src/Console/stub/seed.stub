<?php
/**
 * The {name} seeder
 * 
 * @see https://github.com/fzaninotto/Faker for all documentation
 */
$faker = \Faker\Factory::create();

$seeds = [];

foreach (range(1, {num}) as $key) {
    $seeds[] = [
        'id' => 1,
        'name' => $faker->name,
        'created_at' => $faker->dateTime(),
        'updated_at' => $faker->dateTime()
    ];
}

return ['{name}' => $seeds];
